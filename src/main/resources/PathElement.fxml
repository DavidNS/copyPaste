<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<fx:root type="javafx.scene.control.ListCell"
	xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
	<graphic>
		<AnchorPane prefHeight="25.0"
			xmlns="http://javafx.com/javafx/8.0.171"
			xmlns:fx="http://javafx.com/fxml/1">
			<GridPane AnchorPane.bottomAnchor="0.0"
				AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
				AnchorPane.topAnchor="0.0">
				<columnConstraints>
					<ColumnConstraints hgrow="SOMETIMES"
						maxWidth="1.7976931348623157E308" minWidth="10.0" />
					<ColumnConstraints hgrow="SOMETIMES"
						maxWidth="20.0" minWidth="10.0" prefWidth="20.0" />
				</columnConstraints>
				<rowConstraints>
					<RowConstraints minHeight="10.0" prefHeight="30.0"
						vgrow="SOMETIMES" />
				</rowConstraints>
				<children>
					<AnchorPane prefHeight="200.0">
						<children>
							<Label fx:id="pathLabel" text="Path"
								AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
								AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
						</children>
					</AnchorPane>
					<AnchorPane prefHeight="200.0" prefWidth="200.0"
						GridPane.columnIndex="1">
						<children>
							<Button fx:id="buttonCancel"  mnemonicParsing="false" text="X"
								AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
								AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
						</children>
					</AnchorPane>
				</children>
			</GridPane>
		</AnchorPane>
	</graphic>
</fx:root>